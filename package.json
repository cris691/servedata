{
  "name": "servedata",
  "version": "1.2.2",
  "description": "server for data.",
  "main": "index.js",
  "scripts": {
    "build-views": "npx webpack",
    "dev-db": "export SD_MODE=dev&& nodemon -r esm start.js",
    "db": "export SD_MODE=prod&& node -r esm start.js",
    "dev-site": "./dev_views.sh && npm run pack && npm run dev-db",
    "site": "./prod_views.sh && npm run build-views && npm run pack && npm run db",
    "static": "npm run pack && serve -p 8080 public",
    "dev": "npm run dev-site",
    "start": "npm run site",
    "test-api": "nodemon -r esm tests.js",
    "test": "npm run test-api",
    "pack": "npx snowpack --include \"public/src/**/*.js\" --dest public/src/web_modules",
    "up": "rm package-lock.json && rm -rf node_modules && npm upgrade && npm i && npm run pack"
  },
  "nodemonConfig": {
    "ignore": [
      "db-servedata/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cris691/servedata.git"
  },
  "keywords": [
    "server",
    "data"
  ],
  "author": "@dosy",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cris691/servedata/issues"
  },
  "homepage": "https://github.com/cris691/servedata#readme",
  "dependencies": {
    "beamsplitter": "^2.0.4",
    "bepis": "^2.1.20",
    "cookie-parser": "^1.4.5",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jtype-system": "^1.1.8",
    "nodemailer": "^6.4.6",
    "stubdb": "^1.0.10",
    "style.dss": "^1.3.7"
  },
  "devDependencies": {
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.3",
    "snowpack": "^1.7.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
